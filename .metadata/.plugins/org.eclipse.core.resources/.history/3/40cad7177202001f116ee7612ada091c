package course;

import java.util.Scanner;

public class testeboy {

	public static void main(String[] args) {


		Scanner sc = new Scanner(System.in);
		System.out.println("Escreva algo: ");
		String val = sc.next();
		
		if (val == "y") {
			System.out.println("tamo doido");
		} else {
			System.out.println("tamo muito doido");
		}
 
	}

}
