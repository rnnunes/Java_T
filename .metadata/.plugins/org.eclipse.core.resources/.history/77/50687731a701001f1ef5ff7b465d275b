package entities;

public class Employee {

	public String name;
	public double salGrosso;
	public double tax;
	
	public double salLiq() {
		return salGrosso - tax;
	}
	
	public double aumentoSal(double percentual) {
		double calculo = ((percentual/100) * salGrosso);
		return calculo + salLiq();
	}
}
