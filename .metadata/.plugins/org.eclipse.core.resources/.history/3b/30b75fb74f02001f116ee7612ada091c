package entities;

public class Entities {

	//Atributos
	private int conta;
	private String usuario;
	private String resp;
	private double ini_deposito;
	
	//Construtores
	public Entities(int conta, String usuario, String resp) {
		this.conta = conta;
		this.usuario = usuario;
		this.resp = resp;
		
		
	//Encapsulamento
	public int getConta() {	
	return conta;
	
	}
	
}
